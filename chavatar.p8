--player

function player_update()
		--physics
		player.dy+=gravity
		player.dx*=friction
		
		newx=player.x
		newy=player.y
end


		
function player_movement()
		
		--controls
		if btn(‚¨ÖÔ∏è) then
				player.dx-=player.acc
				player.running=true
				player.flp=true
				player.fight=false
		end
		if btn(‚û°Ô∏è) then
				player.dx+=player.acc
				player.running=true
				player.flp=false
				player.fight=false
		end
		
		--slide
		if player.running
		and not btn(‚¨ÖÔ∏è)
		and not btn(‚û°Ô∏è)
		and not player.falling
		and not player.jumping then
		  player.running=false
		  player.sliding=true
		end
		
		--jump
		if btnp(‚¨ÜÔ∏è)
		and player.landed then
				player.dy-=player.boost
				player.landed=false
		end
		
		--coup de baton
		if btnp(‚ùé)
		and player.landed then
				player.fight=true
		end
		
		interact(newx,newy)
		
--check collision up and down
		if player.dy>0 then
				player.falling=true
				player.landed=false
				player.jumping=false
				
			if collide_map(player,"down",0) then
				  player.landed=true
				  player.falling=false
				  player.dy=0
				  player.y-=(player.y+player.h)%8
				end
		elseif player.dy<0 then
		  player.jumping=true
		  if collide_map(player,"up",1) then
		    player.dy=0
		  end		     	
		end
		
--check player collision left and right
  if player.dx<0 then
    if collide_map(player,"left",1) then
      player.dx=0
    end
  elseif player.dx>0 then
    if collide_map(player,"right",1) then
      player.dx=0
    end
  end
  
--check collisions degats
  if player.invul<=0 then
	 	if collide_map(player,"down",3) then
					  player.life-=1
					  player.dy=0
					  player.invul=30
					  sfx(1)
					  player.y-=(player.y+player.h)%8
					end
	
		 if collide_map(player,"up",3) then
			   player.dy=0	
			   player.life-=1
			   player.invul=30
			   sfx(1)	     	
			end
			
	--check player collision left and right
	  if player.dx<0 then
	    if collide_map(player,"left",3) then
	      player.dx=0
	      player.life-=1
	      player.invul=30
	      sfx(1)
	    end
	  elseif player.dx>0 then
	    if collide_map(player,"right",3) then
	      player.dx=0
	      player.life-=1
	      player.invul=30
	      sfx(1)
	    end
	  end
  else player.invul-=1
  end
  if player.life==0 then
  state=1
  return
  end
--stop sliding
  if player.sliding then
  		if abs(player.dx)<.2
  		or player.running then
  		  player.dx=0
  		  player.sliding=false
  		end
  end  
		
		player.x+=player.dx
		player.y+=player.dy
		
		--limit player to map
		if player.x<map_start then
		  player.x=map_start
		end
		if player.x>map_end-player.w then
		  player.x=map_end-player.w
		end	
end

--interact gemme
function interact(x,y)
 x/=8
 y/=8
	if check_flag(2,x,y) then
		pick_up(x,y)
		sfx(0)
	end
end

function player_animate()

  if player.jumping then
    player.sp=96
  elseif player.falling then
    player.sp=98
  elseif player.sliding then
    player.sp=100
  elseif player.running then
    if time()-player.anim>.1 then
      player.anim=time()
      player.sp+=2
      if player.sp>74 then
        player.sp=68
      end
    end
  elseif player.fight then
  		if time()-player.anim>.1 then      		
  		  player.anim=time()
    		player.sp+=2
    		if player.sp>106 then
      		player.sp=102
     	end
   	end  
  else --player idle
    if time()-player.anim>.5 then
      player.anim=time()
      player.sp+=2
      if player.sp>66 then
        player.sp=64
      end
    end
  end	
end



