function _init()
	player={
									sp=64,
									x=1,
									y=110,
									w=12,
									h=16,
									flp=false,
									dx=0,
									dy=0,
									max_dx=33,
									max_dy=34,
									acc=0.5,
									boost=4,
									anim=0,
									running=false,
									jumping=false,
									falling=false,
									sliding=false,
									landed=false,
									fight=false,
									life=9,
									invul=0,
									gemme_feu=0
						}
						
						gravity=0.3
						friction=0.85
						
						--simple camera
						cam_x=0
						
						--map limits
						map_start=0
						map_end=1024
						
						state=0
end

function _update()
		player_update()
		player_movement()
  player_animate()
  update_camera()
  interact(newx,newy)
  if (state==1) update_gameover()
end

function _draw()
		cls()
		map(0,0)
		spr(player.sp,player.x,player.y,2,2,player.flp)
		draw_ui()
		draw_life()
		if (state==1) draw_gameover()
end


