--update camera

function update_camera()
		cam_x=player.x-55+(player.w/2)
		if cam_x<map_start then
		  cam_x=map_start
		end
		if cam_x>map_end-128 then
		  cam_x=map_end-128
		end
		
		camera(cam_x,0)
end
		



