function draw_ui()
	camera(0,0)
	spr(128,2,2)
	print_outline("x"..player.gemme_feu,12,4)
end

function print_outline(text,x,y)
 print(text,x-1,y,0)
 print(text,x+1,y,0)
 print(text,x,y-1,0)
 print(text,x,y+1,0)
 print(text,x,y,7)
end

--ui life

function draw_life()
	camera(0,0)
	spr(44,2,15)
	print_outline("x"..player.life,12,15)
end

function print_outline(text,x,y)
 print(text,x-1,y,0)
 print(text,x+1,y,0)
 print(text,x,y-1,0)
 print(text,x,y+1,0)
 print(text,x,y,7)
end
