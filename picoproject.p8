function _update()
	if scene=="menu" then
			update_menu()
	elseif scene=="game" then
			update_game()		
	end
end

function _draw()
	if scene=="menu" then
			draw_menu()
		elseif scene=="game" then
			draw_game()
	end
end
-->8
function update_menu()
	if btnp(❎) then
			scene="game"
	end
end

function update_game()
		if btn(⬅️) then x-=1 end
		if	btn(➡️) then x+=1 end
		if btn(⬆️) then y-=1 end
		if btn(⬇️) then y+=1 end
		
		if btnp(❎) then 
			scene="menu"
	end
end
-->8
function draw_menu()
		cls()
		print("press ❎ to start",30,63)
		

end

function draw_game()
		cls()
		spr(1,x,y)
end
